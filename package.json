{
    "name": "portfolio",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "chalk": "^2.4.1",
        "dotenv": "^6.2.0",
        "fastify": "^1.13.3",
        "fastify-compress": "^0.7.1",
        "fastify-helmet": "^3.0.0",
        "fastify-static": "^1.0.0",
        "glob": "^7.1.3",
        "node-sass": "^4.11.0",
        "react": "^16.7.0",
        "react-dom": "^16.7.0-alpha.2",
        "react-helmet": "^5.2.0",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-scripts": "2.1.1",
        "react-snap": "^1.23.0",
        "sw-precache": "^5.2.1"
    },
    "devDependencies": {
        "prettier": "^1.15.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "postbuild": "react-snap && yarn run sw-precache",
        "sw-precache": "sw-precache --config=src/ServiceWorker/sw-precache-config.js",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prettier": "./node_modules/.bin/prettier --tab-width 4 --print-width 120 --write \"+(tests|src)/**/**.+(js|jsx|scss)\""
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "reactSnap": {
        "skipThirdPartyRequests": true
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
