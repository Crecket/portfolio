{
    "name": "portfolio",
    "version": "0.1.0",
    "homepage": "https://gregoryg.dev/",
    "license": "MIT",
    "scripts": {
        "pm2": "pm2 start pm2.yml",
        "start": "node build-server/Server.js",
        "dev": "nodemon --exec babel-node server/Server.js",
        "react": "react-scripts start",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prebuild": "yarn run generate-bunq-data",
        "build": "yarn run build:server && yarn run build:client",
        "build:server": "babel server --out-dir build-server --verbose",
        "build:client": "react-scripts build",
        "postbuild:client": "react-snap && yarn run sw-precache",
        "sw-precache": "sw-precache --config=src/ServiceWorker/sw-precache-config.js",
        "prettier": "./node_modules/.bin/prettier --tab-width 4 --print-width 120 --write \"+(tests|src|server)/**/**.+(js|jsx|css|scss)\"",
        "generate-bunq-data": "babel-node scripts/generate-bunq-data.js"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "chalk": "^2.4.1",
        "chart.js": "^2.8.0",
        "chartjs-plugin-datalabels": "^0.6.0",
        "dotenv": "^6.2.0",
        "fastify": "^2.1.0",
        "fastify-auth": "^0.4.0",
        "fastify-compress": "^0.8.1",
        "fastify-helmet": "^3.0.0",
        "fastify-static": "^2.3.4",
        "react": "^16.8.5",
        "react-chartjs-2": "^2.7.4",
        "react-dom": "^16.8.5",
        "react-helmet": "^5.2.0",
        "react-particles-webgl": "^0.1.11",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^2.1.8",
        "react-use": "^5.1.1",
        "three": "^0.102.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
        "@babel/plugin-transform-async-to-generator": "^7.3.4",
        "@babel/preset-env": "^7.2.3",
        "@bunq-community/bunq-js-client": "^0.40.0",
        "@material-ui/core": "^3.9.2",
        "@material-ui/icons": "^3.0.2",
        "glob": "^7.1.3",
        "http-server": "^0.11.1",
        "json-store": "^1.0.0",
        "node-sass": "^4.11.0",
        "prettier": "^1.15.3",
        "react-snap": "^1.23.0",
        "sw-precache": "^5.2.1"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "reactSnap": {
        "skipThirdPartyRequests": true
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
