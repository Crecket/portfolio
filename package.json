{
    "name": "portfolio",
    "version": "0.1.0",
    "homepage": "https://gregoryg.dev/",
    "private": true,
    "scripts": {
        "start": "react-scripts start",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prebuild": "yarn run generate-bunq-data",
        "build": "react-scripts build",
        "postbuild": "react-snap && yarn run sw-precache",
        "predeploy": "yarn run build",
        "deploy": "gh-pages -b gh-pages -d build",
        "sw-precache": "sw-precache --config=src/ServiceWorker/sw-precache-config.js",
        "prettier": "./node_modules/.bin/prettier --tab-width 4 --print-width 120 --write \"+(tests|src)/**/**.+(js|jsx|scss)\"",
        "generate-bunq-data": "babel-node scripts/generate-bunq-data.js"
    },
    "dependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
        "@babel/plugin-transform-async-to-generator": "^7.2.0",
        "@babel/preset-env": "^7.2.3",
        "@material-ui/core": "^3.9.2",
        "@material-ui/icons": "^3.0.2",
        "axios": "^0.18.0",
        "chalk": "^2.4.1",
        "chart.js": "^2.8.0",
        "dotenv": "^6.2.0",
        "gh-pages": "^2.0.1",
        "glob": "^7.1.3",
        "node-sass": "^4.11.0",
        "react": "^16.7.0-alpha.2",
        "react-chartjs-2": "^2.7.4",
        "react-dom": "^16.7.0-alpha.2",
        "react-helmet": "^5.2.0",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^2.1.5",
        "react-snap": "^1.23.0",
        "react-use": "^5.1.1",
        "sw-precache": "^5.2.1"
    },
    "devDependencies": {
        "@bunq-community/bunq-js-client": "^0.40.0",
        "json-store": "^1.0.0",
        "prettier": "^1.15.3"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "reactSnap": {
        "skipThirdPartyRequests": true
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
