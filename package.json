{
    "name": "portfolio",
    "version": "0.1.0",
    "homepage": "https://gregoryg.dev/",
    "license": "MIT",
    "scripts": {
        "release": "bash scripts/release.sh",
        "pm2": "pm2 start pm2.yml",
        "start": "node build-server/Server.js",
        "dev": "nodemon --exec babel-node server/Server.js",
        "react": "react-app-rewired start",
        "test": "react-app-rewired test",
        "eject": "react-scripts eject",
        "build": "yarn run build:server && yarn run build:client",
        "build:server": "babel server --out-dir build-server --verbose",
        "build:client": "react-app-rewired build",
        "postbuild:client": "react-snap && yarn run sw-precache",
        "prettier": "./node_modules/.bin/prettier --tab-width 4 --print-width 120 --write \"+(tests|scripts|src|server)/**/**.+(js|jsx|css|scss)\"",
        "sw-precache": "sw-precache --config=src/ServiceWorker/sw-precache-config.js",
        "optimize-images": "babel-node scripts/optimize-images.js",
        "generate-bunq-data": "babel-node scripts/generate-bunq-data.js",
        "generate-chart-images": "babel-node scripts/generate-chart-images.js"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "chalk": "^2.4.1",
        "chartjs-node-canvas": "^2.4.0",
        "dotenv": "^6.2.0",
        "fastify": "^2.1.0",
        "fastify-auth": "^0.4.0",
        "fastify-compress": "^0.8.1",
        "fastify-helmet": "^3.0.0",
        "fastify-static": "^2.3.4",
        "sitemap": "^2.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/node": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
        "@babel/plugin-transform-async-to-generator": "^7.3.4",
        "@babel/preset-env": "^7.2.3",
        "@bunq-community/bunq-js-client": "^0.40.0",
        "@material-ui/core": "^4.0.0-beta.1",
        "@material-ui/icons": "^4.0.0-beta.0",
        "@material-ui/lab": "^4.0.0-alpha.11",
        "awaiting": "^3.0.0",
        "chart.js": "^2.8.0",
        "chartjs-plugin-annotation": "^0.5.7",
        "chartjs-plugin-datalabels": "^0.6.0",
        "classnames": "^2.2.6",
        "fs-extra": "^8.0.1",
        "glob": "^7.1.3",
        "gzip-size": "^5.1.0",
        "http-server": "^0.11.1",
        "imagemin": "^6.1.0",
        "imagemin-pngquant": "^7.0.0",
        "json-store": "^1.0.0",
        "loadable-components": "2.2.3",
        "node-sass": "^4.12.0",
        "prettier": "^1.17.1",
        "qrcode.react": "^0.9.3",
        "react": "^16.8.5",
        "react-app-rewired": "^2.1.3",
        "react-chartjs-2": "^2.7.4",
        "react-dom": "^16.8.5",
        "react-helmet": "^6.0.0-beta",
        "react-loadable": "^5.5.0",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^3.0.1",
        "react-snap": "^1.23.0",
        "react-tilt": "^0.1.4",
        "react-use": "^5.1.1",
        "sharp": "^0.22.1",
        "sw-precache": "^5.2.1",
        "three": "^0.102.1"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "reactSnap": {
        "skipThirdPartyRequests": true,
        "http2PushManifest": true,
        "concurrency": 6,
        "inlineCss": true
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
